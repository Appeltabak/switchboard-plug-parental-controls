#: ../src/Translation.vala:23
msgid "Manage parental control settings"
msgstr "ניהול הגדרות בקרת הורים"

#: ../src/Translation.vala:24
msgid "Authentication is required to change parental control settings"
msgstr "נדרש אימות על מנת לשנות הגדרות בקרת הורים"

#: ../src/Widgets/AppChooser.vala:71
msgid "Search Applications"
msgstr "חיפוש יישומים"

#: ../src/Widgets/UserList.vala:42
msgid "My Account"
msgstr "החשבון שלי"

#: ../src/Widgets/UserList.vala:46
msgid "Other Accounts"
msgstr "חשבונות אחרים"

#: ../src/Widgets/GeneralBox.vala:62
msgid "Allow this user to:"
msgstr "מותר למשתמש זה:"

#: ../src/Widgets/GeneralBox.vala:65
msgid "Modify the dock"
msgstr "שינוי המעגן"

#: ../src/Widgets/GeneralBox.vala:68
msgid "Configure printing"
msgstr "הגדרת הדפסה"

#: ../src/Widgets/GeneralBox.vala:84
msgid "Limit computer use:"
msgstr "הגבלת השימוש במחשב:"

#: ../src/Widgets/GeneralBox.vala:92
msgid "On weekdays and weekends"
msgstr "בימות השבוע ובסופי שבוע"

#: ../src/Widgets/GeneralBox.vala:93
msgid "Only on weekdays"
msgstr "בימות השבוע בלבד"

#: ../src/Widgets/GeneralBox.vala:94
msgid "Only on weekends"
msgstr "בסופי שבוע בלבד"

#: ../src/Widgets/GeneralBox.vala:102
msgid "Weekdays"
msgstr "ימות השבוע"

#: ../src/Widgets/GeneralBox.vala:106
msgid "Weekends"
msgstr "סופי שבוע"

#: ../src/Widgets/WeekSpinBox.vala:44
msgid "From:"
msgstr "מ־:"

#: ../src/Widgets/WeekSpinBox.vala:46
msgid "To:"
msgstr "עד:"

#: ../src/Widgets/ControlPage.vala:74
msgid "General"
msgstr "כללי"

#: ../src/Widgets/ControlPage.vala:75
msgid "Internet"
msgstr "אינטרנט"

#: ../src/Widgets/ControlPage.vala:76
msgid "Applications"
msgstr "יישומים"

#: ../src/Widgets/AppsBox.vala:97
msgid "Prevent %s from using these apps:"
msgstr "%s מנוע מלהשתמש ביישומונים אלו:"

#: ../src/Widgets/AppsBox.vala:108
msgid "Add Prevented Apps…"
msgstr "הוספת יישומונים למניעה..."

#: ../src/Widgets/AppsBox.vala:112
msgid "Remove Selected App"
msgstr "הסרת היישומון הנבחר"

#: ../src/Widgets/AppsBox.vala:117
msgid "Clear All"
msgstr "ניקוי הכול"

#: ../src/Widgets/AppsBox.vala:140
msgid "Allow access to these apps with admin permission:"
msgstr "מתן אפשרות גישה ליישומונים אלו עם הרשאות מנהל:"

#: ../src/Widgets/InternetBox.vala:78
msgid "Prevent %s from visiting these websites:"
msgstr "%s מנוע מלבקר באתרי האינטרנט הבאים:"

#: ../src/Widgets/InternetBox.vala:91
msgid "Add URL"
msgstr "הוספת כתובת"

#: ../src/Widgets/InternetBox.vala:100
msgid "Add a new URL, for example: google.com"
msgstr "הוספת כתובת חדשה, לדוגמה: google.com"

#: ../src/Widgets/InternetBox.vala:101
msgid "Invalid URL"
msgstr "כתובת לא חוקית"

#: ../src/Plug.vala:72
msgid "Some settings require administrator rights to be changed"
msgstr "הגדרות מסוימות דורשות הרשאות מנהל לשינוי"

#: ../src/Plug.vala:81
msgid "No users to edit"
msgstr "אין משתמשים לעריכה"

#: ../src/Plug.vala:81
msgid ""
"Parental Controls can only be applied to user accounts that don't have "
"administrative permissions.\n"
"You can change a user's account type from \"Administrator\" to \"Standard\" "
"in the User Accounts pane."
msgstr ""
"ניתן ליישם בקרת הורים רק עבור משתמשים שאינם בעלי הרשאות מנהל.\n"
"ניתן לשנות את סוג המשתמש בלוח חשבונות משתמשים."

#: ../src/Plug.vala:84
msgid "Configure User Accounts"
msgstr "הגדרת חשבונות משתמשים"

#: ../src/Plug.vala:87
msgid "Open Users settings"
msgstr "פתיחת הגדרות משתמשים"

#: ../src/Plug.vala:164
msgid "Parental Control"
msgstr "בקרת הורים"

#: ../src/Plug.vala:165
msgid "Configure time limits and restrict application usage"
msgstr "הגדרת מגבלות זמן והגבלת השימוש ביישומים"

#: ../src/client/Client.vala:125
msgid ""
"Make sure to close all applications before your computer will be locked."
msgstr "נא לוודא שכל היישומים נסגרו לפני שהמחשב ינעל."

#: ../src/client/Client.vala:129
msgid "%ld hour"
msgid_plural "%ld hours"
msgstr[0] "שעה אחת"
msgstr[1] "%ld שעות"

#: ../src/client/Client.vala:133
msgid "%ld minute"
msgid_plural "%ld minutes"
msgstr[0] "דקה אחת"
msgstr[1] "%ld דקות"

#: ../src/client/Client.vala:136
msgid "This computer will lock after %s %s. %s"
msgstr "המחשב ינעל לאחר %s %s. %s"

#: ../src/client/Client.vala:138
msgid "Time left"
msgstr "זמן שנותר"

#: ../src/client/AppLockDialog.vala:27
msgid "You cannot run this application"
msgstr "לא ניתן להריץ יישום זה"

#: ../src/client/AppLockDialog.vala:28
msgid "You are not permitted to run this application."
msgstr "אין הרשאה להריץ יישום זה."
