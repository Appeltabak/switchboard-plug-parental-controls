msgid "%ld hour"
msgid_plural "%ld hours"
msgstr[0] "%ld godzina"
msgstr[1] "%ld godziny"
msgstr[2] "%ld godzin"

#: ../src/client/Client.vala:133
msgid "%ld minute"
msgid_plural "%ld minutes"
msgstr[0] "%ld minuta"
msgstr[1] "%ld minuty"
msgstr[2] "%ld minut"

#: ../src/client/Client.vala:136
msgid "This computer will lock after %s %s. %s"
msgstr "Ten komputer zostanie zablokowany po %s %s. %s"

#: ../src/client/Client.vala:138
msgid "Time left"
msgstr "Pozostały czas"

#: ../src/client/AppLockDialog.vala:27
msgid "You cannot run this application"
msgstr "Nie możesz uruchomić tego programu"

#: ../src/client/AppLockDialog.vala:28
msgid "You are not permitted to run this application."
msgstr "Nie masz uprawnień do uruchomienia tego programu."
